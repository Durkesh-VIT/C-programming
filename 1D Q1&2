#include <stdio.h>

int main()
{
    int arr[5];
    int n; // number of element user wants 
    arr[0]=1;  // insert one element to the array 
    printf("Enter the number of elements you want to append ( less than 4) ");
    scanf("%d",&n);
     
    for(int i=0;i<n;i++){
        
        printf("Enter the element arr[%d]:",i+1);
        scanf("%d",&arr[i+1]);
    }
    
    return 0;
}
